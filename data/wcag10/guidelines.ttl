  wcag:id "1.4" ;
  wcag:priority <http://www.w3.org/TR/WCAG10/#wc-priority-1> ;
  dct:title "For any time-based multimedia presentation (e.g., a movie or animation), synchronize equivalent alternatives (e.g., captions or auditory descriptions of the visual track) with the presentation."@en ;
  # http://www.w3.org/TR/WAI-WEBCONTENT-TECHS/#tech-synchronize-equivalents
  dct:hasPart <http://www.w3.org/TR/WCAG10-CORE-TECHS/#audio-information> ;
  dct:hasPart <http://www.w3.org/TR/WCAG10-HTML-TECHS/#accessible-applets> .

wcag10:tech-redundant-client-links
  a earl:TestRequeriment, wcag:Checkpoint ;
  dct:isPartOf wcag10:gl-provide-equivalents ;
  wcag:id "1.5"; 
  wcag:priority <http://www.w3.org/TR/WCAG10/#wc-priority-3> ;
  dct:title "[Until user agents](http://www.w3.org/TR/WCAG10/wai-pageauth.html#until-user-agents) render text equivalents for client-side image map links, provide redundant text links for each active region of a client-side image map."@en ;
  dct:references wcag10:tech-redundant-server-links ;
  dct:references wcag10:tech-client-side-maps ;
  dct:conformsTo wcag10:until-user-agents ;
