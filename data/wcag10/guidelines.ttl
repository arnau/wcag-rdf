  wcag:priority <http://www.w3.org/TR/WCAG10/#wc-priority-3> ;
  dct:title "[Until user agents](http://www.w3.org/TR/WCAG10/wai-pageauth.html#until-user-agents) handle empty controls correctly, include default, place-holding characters in edit boxes and text areas."@en ;
  dct:description "For example, in HTML, do this for `textarea` and `input`."@en ;
  dct:conformsTo wcag10:until-user-agents ;
  # http://www.w3.org/TR/WAI-WEBCONTENT-TECHS/#tech-place-holders
  dct:hasPart <http://www.w3.org/TR/WCAG10-HTML-TECHS/#forms-specific> .

wcag10:tech-divide-links
  a earl:TestRequeriment, wcag:Checkpoint ;
  dct:isPartOf wcag10:gl-interim-accessibility ;
  wcag:id "10.5"; 
  wcag:priority <http://www.w3.org/TR/WCAG10/#wc-priority-3> ;
  dct:title "[Until user agents](http://www.w3.org/TR/WCAG10/wai-pageauth.html#until-user-agents) (including assistive technologies) render adjacent links distinctly, include non-link, printable characters (surrounded by spaces) between adjacent links."@en ;
