  a earl:TestRequeriment, wcag:Checkpoint ;
  dct:isPartOf wcag10:gl-facilitate-comprehension ;
  wcag:id "14.3"; 
  wcag:priority <http://www.w3.org/TR/WCAG10/#wc-priority-3> ;
  dct:title "Create a style of presentation that is consistent across pages."@en ;
  # http://www.w3.org/TR/WAI-WEBCONTENT-TECHS/#tech-consistent-style
  dct:hasPart <http://www.w3.org/TR/WCAG10-CORE-TECHS/#navigation> ;
  dct:hasPart <http://www.w3.org/TR/WCAG10-CSS-TECHS/#consistency> .

